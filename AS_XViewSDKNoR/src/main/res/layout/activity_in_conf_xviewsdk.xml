<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black_xviewsdk"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/view_conf_videolist_xviewsdk"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

    <include
        android:id="@+id/conf_control_xviewsdk"
        layout="@layout/activity_conf_xviewsdk" />

    <com.cinlan.xview.widget.SlidingLayer
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/right_sliding_layer_xviewsdk"
        android:layout_width="match_parent"
        android:focusable="false"
        android:clickable="false"
        android:layout_height="match_parent" >
    </com.cinlan.xview.widget.SlidingLayer>

</FrameLayout>